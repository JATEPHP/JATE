doctype html
html(lang="en")
	head
		base(href="_base_")
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible" content="IE=edge")
		meta(name="viewport" content="width=device-width, initial-scale=1")
		meta(name="description" content="<_metaDescription_>")
		meta(name="keywords" content="<_metaKeywords_>")
		meta(name="author" content="<_metaAuthor_>")
		meta(name="robots" content="index, follow")
		meta(property="og:url" content="http://www.JATE.it/")
		meta(property="og:type" content="website")
		meta(property="og:title" content="<_title_>")
		meta(property="og:description" content="<_metaDescription_>")
		meta(property="og:image" content="img/favicon.png")
		link(rel="shortcut icon" type="image/png" href="img/favicon.png")
		noscript Your browser does not support JavaScript!
		title <_title_>
		<_css_>
	body
		nav(class="navbar navbar-default navbar-inverse navbar-fixed-top" role="navigation")
			.container-fluid
				.navbar-header
					button(type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false")
						span(class="sr-only" Toggle navigation)
						span(class="icon-bar")
						span(class="icon-bar")
						span(class="icon-bar")
					.navbar-brand
						<_brand_>
				#navbar-collapse.collapse.navbar-collapse
					ul(class="nav navbar-nav navbar-right")
						<_menu_>
		section(class="col-lg-12" id="page")
			<_content_>
		footer(class="col-lg-12" id="footer")
			<_footer_>
		<_outContent_>
		<_js_>
		<_jsVariables_>
